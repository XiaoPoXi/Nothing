source 'https://github.com/CocoaPods/Specs.git'

platform:ios,'10.0'
target ‘Nothing’ do
  use_frameworks!
  pod 'AFNetworking'
  pod 'MBProgressHUD'
  pod 'MJRefresh'
#  pod 'SDWebImage'
  pod 'RAMAnimatedTabBarController' #定制TabBar动画
  pod 'SnapKit'    #Swift中的Marsony
  pod 'Kingfisher' #喵神的纯swift编写的图片处理框架，类似SDWebImage
  pod 'HandyJSON' #处理model的
  pod 'SwiftyJSON' #处理model的
  pod 'IQKeyboardManagerSwift'
  pod 'Reusable' #https://www.jianshu.com/p/255e02337176/ Reusable是一个在swift下使用的开源库。利用protocol extension结合泛型提供了一个优雅的方案来dequeueReusableCell。
  pod 'Then'



end
